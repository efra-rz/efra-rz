  body.bodyBg
    header.main-header__topbar
      .ed-container
        // Start logo
        .ed-item.s-100.lm-40.cross-center.logo-align
          a(href="/"): img(src='img/logo-web.png', alt='')
        // End logo
        
        // Start menu
        nav.ed-item.s-100.lm-60.cross-center
          //ul.ed-menu.l-horizontal
          ul.main-menu.ed-menu.m-horizontal
            block inicio
              li.main-menu__item: a(href='/').main-menu__link Inicio
            block acerca
              li.main-menu__item: a(href='/hola.html').main-menu__link Hola
            block servicios
              li.main-menu__item: a(href='/servicios.html').main-menu__link Servicios
            block proyectos
              li.main-menu__item: a(href='/proyectos.html').main-menu__link Proyectos
            block blog
              li.main-menu__item: a(href='/blog.html').main-menu__link Blog
            block contacto
              li.main-menu__item: a(href='/contacto.html').main-menu__link Contacto
        // End menu
    
    //********************
    //*Start main content*
    //********************
    
    block main

    //*******************
    //****End content****
    //*******************
    
    // Start footer
    footer.main-footer--bak-container
      // Start footer main container
      article.ed-container
        // Start footer about
        section.ed-item.s-100.m-500.m-1-3
          h1 Acerca
          img(src='img/logo-web.png', alt='')
          p.footer-description
            | Hola, soy programador y desarrollador web independiente orientado en tecnologías Frontend (HTML, CSS, JS) y me encuentro ubicado en Mty, N.L. México.
          ul.footer__social-icons
            li: a(href="https://www.facebook.com/efradesarrollo/" target="_blank"): img(src="img/if_facebook_48.png", alt="" width="40px")
            li: a(href="skype:effrix?chat" target="_blank"): img(src="img/if_skype_48.png", alt="" width="40px")
            li: a(href="https://github.com/efrix" target="_blank"): img(src="img/if_github_48.png", alt="" width="40px")
            li: a(href="https://telegram.me/efrix396" target="_blank"): img(src="img/if_telegram_48.png", alt="" width="40px")
            li: a(href="https://api.whatsapp.com/send?phone=5218184602228&text=Hi, I contacted you Through your website." target="_blank"): img(src="img/if_whatsapp_48.png", alt="" width="40px")
            
